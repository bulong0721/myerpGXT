<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container">
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="logoMargins">
		<ui:attributes top="2" right="4" bottom="2" left="4" />
	</ui:with>
	<ui:with type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData" field="logoLayoutData">
		<ui:attributes margins="{logoMargins}" />
	</ui:with>
	<container:HorizontalLayoutContainer>
		<container:child layoutData="{logoLayoutData}">
			<g:Image url="images/icons/AdempiereHR.png" />
		</container:child>
	</container:HorizontalLayoutContainer>
</ui:UiBinder> 