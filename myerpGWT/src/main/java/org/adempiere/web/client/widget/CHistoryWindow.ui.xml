<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button" xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client" xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="i18n" type="org.adempiere.web.client.Messages" />
	<gxt:Window ui:field="window" pixelSize="450, 60" modal="true" blinkModal="true" headingText="{i18n.histroyBar_Ask}">
		<gxt:button>
			<button:TextButton ui:field="dayButton" text="{i18n.histroyBar_Day}" />
		</gxt:button>
		<gxt:button>
			<button:TextButton ui:field="weekButton" text="{i18n.histroyBar_Week}" />
		</gxt:button>
		<gxt:button>
			<button:TextButton ui:field="monthButton" text="{i18n.histroyBar_Month}" />
		</gxt:button>
		<gxt:button>
			<button:TextButton ui:field="yearButton" text="{i18n.histroyBar_Year}" />
		</gxt:button>
		<gxt:button>
			<button:TextButton ui:field="allButton" text="{i18n.histroyBar_All}" />
		</gxt:button>
	</gxt:Window>
</ui:UiBinder> 