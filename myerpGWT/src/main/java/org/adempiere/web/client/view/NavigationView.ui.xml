<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:component="urn:import:org.adempiere.web.client.component" xmlns:app="urn:import:org.adempiere.web.client.apps">
	<ui:with type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData" field="mainLayoutData">
		<ui:attributes width="1" height="1" />
	</ui:with>
	<container:VerticalLayoutContainer>
		<container:child layoutData="{mainLayoutData}">
			<app:TreePanel ui:field="treepanel" />
		</container:child>
	</container:VerticalLayoutContainer>
</ui:UiBinder> 