<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:tabs="urn:import:com.sencha.gxt.widget.core.client"
	xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client" xmlns:grid="urn:import:com.sencha.gxt.widget.core.client.treegrid"
	xmlns:toolbar="urn:import:com.sencha.gxt.widget.core.client.toolbar"
	xmlns:component="urn:import:org.adempiere.web.client.component"
	xmlns:widget="urn:import:org.adempiere.web.client.widget" xmlns:form="urn:import:com.sencha.gxt.widget.core.client.form"
	xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button">

	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="marginsSimple">
		<ui:attributes top="4" right="4" bottom="4" left="4" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData"
		field="toolLayoutData">
		<ui:attributes height="-1" width="1" margins="{marginsSimple}" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData"
		field="simpleLayout">
		<ui:attributes width="1" height="1" margins="{marginsSimple}" />
	</ui:with>
	<ui:with field="i18n" type="org.adempiere.web.client.Messages" />

	<gxt:Window ui:field="window" pixelSize="640, 480" modal="true">
		<container:VerticalLayoutContainer>
			<container:child layoutData="{simpleLayout}">
			</container:child>
			<container:child layoutData="{toolLayoutData}">
				<widget:ConfirmToolBar ui:field="toolBar" />
			</container:child>
		</container:VerticalLayoutContainer>
	</gxt:Window>
</ui:UiBinder> 