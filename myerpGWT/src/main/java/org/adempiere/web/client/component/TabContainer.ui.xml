<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container">
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="margins4">
		<ui:attributes top="0" right="0" bottom="0" left="0" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="margins_4">
		<ui:attributes top="4" right="4" bottom="0" left="-4" />
	</ui:with>
	<ui:with type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData" field="bodyLayoutData">
		<ui:attributes width="1" height="1" margins="{margins4}" />
	</ui:with>
	<ui:with type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData" field="tabLayoutData">
		<ui:attributes width="165" height="1" margins="{margins4}" />
	</ui:with>
	<ui:with type="com.sencha.gxt.widget.core.client.container.BoxLayoutContainer.BoxLayoutData" field="barLayoutData">
		<ui:attributes margins="{margins_4}" />
	</ui:with>
	<ui:style type="org.adempiere.web.client.component.TabContainer.ADTabpanelStyle" field="panelStyle">
		.pushButton td div {
			text-align: left !important;
		}
	</ui:style>
	<container:HorizontalLayoutContainer ui:field="container">
		<container:child layoutData="{bodyLayoutData}">
			<container:CardLayoutContainer ui:field="bodyContainer" borders="true">
			</container:CardLayoutContainer>
		</container:child>
		<container:child layoutData="{tabLayoutData}">
			<container:VBoxLayoutContainer ui:field="tabContainer" borders="true">
			</container:VBoxLayoutContainer>
		</container:child>
	</container:HorizontalLayoutContainer>
</ui:UiBinder> 