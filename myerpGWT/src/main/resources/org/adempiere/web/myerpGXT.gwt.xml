<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="myerpGXT">
	<stylesheet src="reset.css" />
	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name="com.google.gwt.user.User" />
	<inherits name="javax.validation.Validation" />
	<inherits name="com.google.gwt.editor.Editor" />
	<inherits name="com.google.gwt.logging.Logging" />
	<inherits name="com.sencha.gxt.ui.GXT" />
	<inherits name="com.sencha.gxt.theme.gray.Gray" />
	<inherits name="org.adempiere.Model" />
	<inherits name="com.mvp4g.Mvp4gModule" />
	<source path="client" />
	<source path="shared" />
	<entry-point class='com.mvp4g.client.Mvp4gEntryPoint' />
	<generate-with class="org.adempiere.web.client.util.ClassActivatorGenerator">
		<when-type-assignable class="org.adempiere.web.client.util.IClassActivator" />
	</generate-with>
	<set-property name="user.agent" value="safari" />
	<add-linker name="xsiframe" />
	<set-configuration-property name="devModeRedirectEnabled" value="true" />
	<set-property name="compiler.useSourceMaps" value="false" />
	<set-property name="ClientBundle.enableInlining" value="false" />
	<extend-property name="locale" values="en,zh_CN" />
	<set-property-fallback name="locale" value="zh_CN" />
	<set-configuration-property name="locale.searchorder" value="cookie,useragent" />
	<set-configuration-property name="locale.cookie" value="lang" />
</module>
