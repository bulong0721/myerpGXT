<?xml version="1.0" encoding="UTF-8"?>

<web-app>

	<!-- Servlets -->

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>myerpGXT.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>reportServlet</servlet-name>
		<servlet-class>org.adempiere.web.server.ReportDistributeServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>reportServlet</servlet-name>
		<url-pattern>/myerpGXT/report</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>adempiereServlet</servlet-name>
		<servlet-class>org.adempiere.web.server.AdempiereServiceImpl
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>adempiereServlet</servlet-name>
		<url-pattern>/myerpGXT/adempiere</url-pattern>
	</servlet-mapping>

</web-app>
